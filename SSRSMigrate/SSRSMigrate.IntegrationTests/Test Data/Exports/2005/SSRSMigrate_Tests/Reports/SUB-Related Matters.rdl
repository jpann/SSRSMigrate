      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <CommandText>SELECT
  MattersContacts.MattersContacts
  ,MattersContacts.Matters AS [MattersContacts Matters]
  ,MattersContacts.Contacts
  ,MattersContacts.MtoCClass
  ,MattersContacts.IsMain
  ,MattersContacts.IsClient
  ,MattersContacts.IsBill
  ,MattersContacts.BillPercent
  ,MattersContacts.CorrType
  ,MattersContacts.MtoCNotes
  ,MattersContacts.IsImport
  ,MattersContacts.IsStmnCC
  ,MattersContacts.IsNew
  ,MattersContacts.IsClientAgency
  ,Matters.Matters AS [Matters Matters]
  ,Matters.MatterID
  ,Matters.ShortDesc
  ,Matters.AreaOfLaw
  ,Matters.MatterType
  ,Matters.ClientSort
  ,Matters.MatterIDSort
FROM
Matters
INNER JOIN MattersContacts ON MattersContacts.Matters = Matters.Matters
LEFT OUTER JOIN SecurityGroup SGView ON Matters.SecurityGroupView = SGView.SecurityGroup  
  INNER JOIN SecurityILSKey ON Matters.ILSKey = SecurityILSKey.ILSKey 
    WHERE Contacts = @Contacts AND SecurityILSKey.Professionals = @CurrentProfessional</CommandText>
        <QueryParameters>
          <QueryParameter Name="@Contacts">
            <Value>=Parameters!Contacts.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CurrentProfessional">
            <Value>=Parameters!CurrentProfessional.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
